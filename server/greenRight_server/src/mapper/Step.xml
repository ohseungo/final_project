<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="step">
	<select id="selectStepListWithUserId" parameterType="String" resultType="Step">
		select STEPID, USERID, STEPCOUNT, STEPDATE
		from STEP
		where USERID = #{id}
		order by STEPDATE
	</select>
</mapper>