<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="company">
	<select id="selectCompanyWithCompId" parameterType="String"
		resultType="Company">
		select COMPID,COMPNAME,COMPTYPE
		from COMPANY 
		where COMPID = #{id}
	</select>
</mapper>